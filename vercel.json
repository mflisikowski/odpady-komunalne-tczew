{
    "version": 2,
    "builds": [
        {
            "src": "packages/backend/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "packages/frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/",
            "dest": "packages/backend/dist/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "packages/frontend/output/$1"
        }
    ]
}